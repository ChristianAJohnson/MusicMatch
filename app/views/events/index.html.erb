<div id="carouselExampleSlidesOnly" class="carousel slide" data-ride="carousel">
  <div class="carousel-inner">
    <div class="carousel-item active">
      <img class="d-block w-100" src="https://msps.blob.core.windows.net/mspsmedia/2017/06/DR_microsoft_kinect_neon_indian_MG_3679-5956db984e6f5.jpg" height="600" alt="First slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="http://farm9.staticflickr.com/8471/8100566896_6f547658bb.jpg" height="600" alt="Second slide">
    </div>
    <div class="carousel-item">
      <img class="d-block w-100" src="https://i.ytimg.com/vi/tgV4tZRR7p0/maxresdefault.jpg" height="600" alt="Third slide">
    </div>
  </div>
</div>

<div class="container-fluid">
 <div class="row align-items-start">
  <div class="col-lg-8">
    <img class="icons" src="/assets/ticket.png" height="50" alt="">
    <h1> Latest Events </h1>
  <% @events.each do |e| %>
      <a href="/events/<%= e.id %>"><div><%= e.performer %></div></a>
      <hr>
  <% end %>
</div>
<div class="col">
  <img class="icons" src="/assets/megaphone-10-240.png" height="50" alt="">
  <h1> Social Feed </h1>
  <% @activities.each do |activity| %>
  <div class="activity">
      <%= link_to activity.owner.username, activity.owner if activity.owner %>
       <%= render_activity activity %>
    </div>
  <hr>
  <% end %>
</div>
</div>
</div>
