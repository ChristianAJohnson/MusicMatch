<div class="search-fields">

<div class="search">
<input type="text" class='search-value' value=''name="" size="40" placeholder="search live nation events" >

<input type="submit" class='search-submit' value='search'>

<center>
<div class='results'></div>
<center>

<h3>search meetup requests</h3>

<%= search_form_for @q, url: url_for(controller: 'events', action: 'new') do |f| %>

  <%= f.label :performer_or_location_or_date_cont %>
  <%= f.search_field :performer_or_location_or_date_cont %>

  <%= f.submit %>
<% end %>

<% @events.reverse.take(5).each do |e| %>
    <a href="/events/<%= e.id %>"><div><%= e.performer %></div></a>
    <%= e.location %>
    <%= e.date %>
    <hr>
<% end %>
</div>
</div>

